<% const pageName = typeof page === "undefined" ? "main" : page %>
<% const pageTitle = typeof title === "undefined" ? "" : title %>
<% const comingData = typeof data === "undefined" ? null : data %>
<% const script = typeof pageScript === "undefined" ? [] : pageScript %>
<% const style = typeof pageStyle === "undefined" ? [] : pageStyle %>
<%-include('partials/head', {title: pageTitle, user: comingData, pageStyle: style})%>
<%-include(`pages/${pageName}`, {data: comingData})%>
<%-include('partials/footer', {pageScript: script})%>